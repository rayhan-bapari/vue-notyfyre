{
	"name": "@rayhan-bapari/notyfyre-vue",
	"version": "0.1.0",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		},
		"./style.css": "./dist/style.css"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup src/index.ts --format esm,cjs --dts --clean && node scripts/copy-css.js",
		"typecheck": "vue-tsc --noEmit"
	},
	"peerDependencies": {
		"vue": "^3.4.0"
	},
	"devDependencies": {
		"tsup": "^8.0.0"
	},
	"license": "MIT"
}
